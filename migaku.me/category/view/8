<script type="text/javascript">

var postIds = "60,55,55,";

var morecount = 0;
var maxpostcount = 3;

$(document).ready(function(){
  $('#more').click(function(){
    morecount ++;
    $.ajax({
      type: "POST",
        data: {postIds: postIds, morecount:morecount},
        url: "/category/ajax/8",
        success: function(data, textStatus){
          $("#fiximg").append(data);
          if(maxpostcount < 12 * (morecount + 1)) {
            $("#more").html("");
          }
        },
          error: function(xhr, textStatus, errorThrown){
          }
    });
  });
});
</script>


